{  
    "build": {
      "arduino": {
        "ldscript": "esp32s3_out.ld",
        "memory_type": "qio_qspi"
      },
      "core": "esp32",
      "extra_flags": [
        "-DARDUINO_ESP32S3_SUPERMINI",
        "-DARDUINO_RUNNING_CORE=1",
        "-DARDUINO_EVENT_RUNNING_CORE=1",  
        "-DBOARD_HAS_PSRAM"
      ],
      "f_cpu": "240000000L",
      "f_flash": "80000000L",
      "flash_mode": "qio",
      "hwids": [
        [
          "0x303A",
          "0x1002"
        ]
      ],
      "mcu": "esp32s3",
      "variants_dir": "boards/variants",
      "variant": "esp32s3_supermini"
    },
    "connectivity": [
      "bluetooth",
      "wifi"
    ],
    "debug": {
      "openocd_target": "esp32s3.cfg"
    },
    "frameworks": [
      "arduino",
      "espidf"
    ],
    "monitor": {
      "speed": 115200
    },
    "name": "ESP32-S3 SuperMini",
    "upload": {
      "flash_size": "4MB",
      "maximum_ram_size": 532480,
      "maximum_size": 4194304,
      "require_upload_port": true,
      "wait_for_upload_port": true,
      "use_1200bps_touch": true,
      "speed": 921600
    },
    "url": "https://www.nologo.tech/en/product/esp32/esp32s3supermini/esp32S3SuperMini.html",
    "vendor": "wmnologo"
}
